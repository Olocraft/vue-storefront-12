{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "lint:fix:sfui": {
      "cache": false,
      "outputs": []
    },
    "build:next": {
      "cache": false
    },
    "build:nuxt": {
      "cache": false
    },
    "build:react": {
      "cache": false
    },
    "build:vue": {
      "cache": false
    },
    "build:release": {
      "dependsOn": [
        "build:react", "build:vue"
      ],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:docs": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}
