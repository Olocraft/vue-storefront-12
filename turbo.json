{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": [
        "@storefront-ui/css#build:css",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "lint:fix:sfui": {
      "cache": false,
      "outputs": []
    },
    "build:next": {
      "cache": false
    },
    "build:nuxt": {
      "cache": false
    },
    "build:css": {
      "cache": false
    },
    "build:react": {
      "cache": false,
      "dependsOn": [
        "build:css"
      ]
    },
    "build:css-copy": {
      "cache": false,
      "dependsOn": [
        "build:react"
      ]
    },
    "dev": {
      "cache": false
    },
    "dev:docs": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}
