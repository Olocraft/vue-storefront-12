{
  "name": "@sfui/docs",
  "version": "0.0.0",
  "private": true,
  "main": "index.md",
  "scripts": {
    "dev:docs": "concurrently \"vitepress dev --port=8000\" \"yarn watch-docs\"",
    "build": "yarn update-docs && VITE_DOCS_BASEPATH=/v2/ VITE_DOCS_EXAMPLES_REACT=https://docs.storefrontui.io/v2-react VITE_DOCS_EXAMPLES_VUE=https://docs.storefrontui.io/v2-vue vitepress build",
    "watch-docs": "nodemon",
    "serve": "vitepress serve",
    "update-docs": "yarn node ./utils/createFrameworksDocs"
  },
  "devDependencies": {
    "@sfui/sfui": "*",
    "concurrently": "^7.5.0",
    "tailwindcss": "^3.1.8",
    "vitepress": "^1.0.0-alpha.4",
    "vue": "^3.2.45"
  }
}
