{
  "name": "@sfui/preview-remix",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "yarn generate:css -- --minify && yarn example:css",
    "build:remix": "remix build",
    "dev": "PORT=3002 run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:example.css": "npm run example:css:watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.scss -o ./app/tailwind.css",
    "example:css": "sass --no-source-map ../../../node_modules/@sfui/example-style/index.scss:./app/example.css",
    "example:css:watch": "sass --no-source-map --watch ../../../node_modules/@sfui/example-style/index.scss:./app/example.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "npm-run-all": "^4.1.5",
    "@sfui/example-style": "*"
  },
  "engines": {
    "node": ">=14"
  }
}
