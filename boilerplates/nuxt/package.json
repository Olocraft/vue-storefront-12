{
  "name": "nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watch",
    "test:update-snapshot": "yarn test --u",
    "lint": "yarn lint:script && yarn lint:style",
    "lint.fix": "yarn lint:script.fix && yarn lint:style.fix",
    "lint:script": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:script.fix": "eslint --ext \".js,.vue\" --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"components/**/*.{css,scss,sass,vue}\"",
    "lint:style.fix": "yarn lint:style --fix",
    "storybook": "nuxt storybook",
    "storybook-build": "nuxt storybook build"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/storybook": "^4.3.0",
    "@vue/composition-api": "^1.7.0",
    "core-js": "^3.19.3",
    "fs-extra": "^10.0.1",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0",
    "@vueuse/core": "^9.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.11",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "@vueuse/components": "^8.3.1",
    "@vueuse/nuxt": "^8.3.1",
    "autoprefixer": "^10.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^9.3.0",
    "jest": "^27.5.1",
    "jest-serializer-vue": "^2.0.2",
    "postcss": "^8.4.12",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.4",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "tailwindcss": "^3.0.23",
    "vue-template-compiler": "^2.6.14"
  },
  "engines": {
    "node": "^16.0.0"
  }
}
